{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "ignorePatterns": ["build/"],
    "rules": {
      "no-unused-vars": ["error", {"varsIgnorePattern": "^_"}],
      "react/no-unknown-property": "off",
      "react/prop-types": "off",
      "no-control-regex": "off"
    },

    "globals": {"process": "readonly"}
}
